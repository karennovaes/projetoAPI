<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Citações</title>
</head>
<body>

<h2>Citações<h2>
    <table border="2">
        <thead>
            <tr>
                <td>Citação </td>
                <td>Ano </td>
                <td>Autor </td>
                <td>Ações </td>
                
            </tr>
        </thead>

       
       <tbody>
        <% data.forEach(function(details) { %>
            <tr>
                <td><%= details.citacao %></td>
                <td><%= details.ano %></td>
                <td><%= details.autor %></td>
            <td><a href="/edit/<%= details._id %> /" >Editar</a> / <a href="/delete/<%=details._id %>">Deletar</a></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
        <button><a href="/">Voltar</a></button>
        
</body>
</html>